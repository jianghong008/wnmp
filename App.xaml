<Application x:Class="wnmp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:wnmp"
             StartupUri="pages/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary  Source="style/main.xaml"/>
                <ResourceDictionary  Source="style/MyScrollViewer.xaml"/>
                <ResourceDictionary  Source="style/tabContr.xaml"/>
            </ResourceDictionary.MergedDictionaries>
           
            <ImageSource x:Key="phpLogo">images/php.png</ImageSource>
            <ImageSource x:Key="mysqlLogo">images/mysql1.png</ImageSource>
            <ImageSource x:Key="nginxLogo">images/nginx.png</ImageSource>
        </ResourceDictionary>
    </Application.Resources>
</Application>
